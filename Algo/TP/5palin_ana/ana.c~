#include <string.h>

int ana(char *s1,char *s2)
{
	int i=0, j ;

	if (strlen(s1)!=strlen(s2)) return 0 ;

	for (i=0 ; s1[i] ; i++)
	{
		for (j=0 ; s2[j] ; j++)
			if (s1[i]==s2[j])
			{
				s2[j]=' ' ; // on barre
				break ;     // au suivant
			}	
		if (s2[j]=='\0') return 0 ; // on n'a pas trouvÃ©
	}
	return 1 ;		
}
